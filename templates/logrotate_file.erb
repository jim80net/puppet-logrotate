# File managed by puppet
<%= @log_real %> {
<% if @options -%>
<% @options.each do |opt| -%>
  <%= opt %>
<% end -%>
<% end -%>
<% if @interval -%>
  <%= @interval %>
<% end -%>
<% if @rotation -%>
  rotate <%= @rotation %>
<% end -%>
<% if @size -%>
  size <%= @size %>
<% end -%>
<% if @minsize -%>
  minsize <%= @minsize %>
<% end -%>
<% if @create -%>
  create <%= @create %>
<% end -%>
<% if @archive -%>
  olddir <%= @olddir_real %>
<% end -%>

<% if @postrotate -%>
  postrotate
<% @postrotate.each do |p| -%>
    <%= p %>
<% end -%>
  endscript
<% end -%>

}
